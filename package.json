  "license": "MIT",
  "description": "formly-js for aurelia module which has a Components to help customize and render JavaScript/JSON configured forms. The formly-form Component and the FormlyConfig service are very powerful and bring unmatched maintainability to your application's forms.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formly-js/ng-formly.git"
  },
  "bugs": {
    "url": "https://github.com/formly-js/ng-formly/issues"
  },
  "homepage": "https://github.com/formly-js/ng-formly#readme",
  "devDependencies": {
    "aurelia-types-installer": "^0.2.13",
    "codecov": "^2.2.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jspm": "^0.17.0-beta.42",
    "remap-istanbul": "^0.9.5",
    "tslint": "^5.5.0",
    "typescript": "^2.5.0-dev.20170719"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
